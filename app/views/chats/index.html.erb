<p id="notice"><%= notice %></p>

<h1>Chats</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Content</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chats.each do |chat| %>
      <tr>
        <td><%= chat.name %></td>
        <td><%= chat.content %></td>
        <td><%= chat.image %></td>
       
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Chat', new_room_chat_path %>
